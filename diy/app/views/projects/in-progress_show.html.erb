<article class="project-page">
  <div class="project-content">

    <div class="top-page">
      <div class="user-project-info">
          <%= link_to root_path do %>
            <%= image_tag "http://www.reactiongifs.us/wp-content/uploads/2016/01/fry_clapping_with_food_futurama.gif", class: "user-pic-size"%>
          <% end %>
        <div class="project-title"> Alvin's Wood </div>
        <div class="project-by"> by Alvin time stamp </div>
      </div>
    </div>

    <div class="mid-page">
      <div class="mid-left">
        <div class="project-pictures">
          <%= link_to root_path do %>
          <%= image_tag "https://assets.pcmag.com/media/images/486771-futurama-game-of-drones.jpg?thumb=y&width=810&height=456", class: "main-project-img" %>
          <% end %>
        </div>
        <div class="attach-pictures">
          <%= link_to root_path do %>
          <%= image_tag "https://static.pexels.com/photos/172292/pexels-photo-172292.jpeg", class: "sub-project-img" %>
          <% end %>
          <%= link_to root_path do %>
          <%= image_tag "https://static.pexels.com/photos/139312/pexels-photo-139312.jpeg", class: "sub-project-img" %>
          <% end %>
          <%= link_to root_path do %>
          <%= image_tag "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRFedLwRl4DpUEIpnDSFBSlpFFuYvCd8g8KQ4RwnP1tLgcdSHomSg", class: "sub-project-img" %>
