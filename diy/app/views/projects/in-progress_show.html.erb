          <% end %>
        </div>
      </div>

      <div class="mid-right">
      <% if current_user %>

      <%-# the Save/Unsave button -%>
        <div id="saves-count">
          <%= button_to "Save/unsave", [@project, @project_status], method: :patch, remote: true, params: { project_status: { saves: true } }, id: "save", class: "button flip save"
 %>

 </div>


      <% end %>


        <div>
          <button class="button flip save">
